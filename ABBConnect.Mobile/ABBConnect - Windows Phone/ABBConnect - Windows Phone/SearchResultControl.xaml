<UserControl x:Class="ABBConnect___Windows_Phone.SearchResultControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="50" d:DesignWidth="456">
    
    <Grid x:Name="LayoutRoot" Background="{StaticResource PhoneChromeBrush}" Margin="0,0,0,0">
            <TextBlock x:Name="lblUserName" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Dimitrio Kostopoulos" VerticalAlignment="Top" MouseLeftButtonUp="lblUserName_MouseLeftButtonUp"/>
    </Grid>
</UserControl>